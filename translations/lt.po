#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Programos atnaujinimas"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Laidos informacija:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Praleisti šią versiją"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Priminti vėliau"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Įdiegti atnaujinimą"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s negali būti perkrauta."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Įsitikinkite, kad neturite neišsaugotų dokumentų ir bandykite dar kartą."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Diegimo programos paleidimas..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Nepavyko paleisti diegimo programos."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Ieškoma atnaujinimų..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Atsisakyti"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Atnaujinimų nėra!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "Jūsų naudojama %s %s versija yra naujausia."

#: src/ui.cpp:774
msgid "Close"
msgstr "Užverti"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Atnaujinimo klaida!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Ieškant informacijos apie atnaujinimą įvyko klaida. Patikrinkite interneto ryšį ir pabandykite dar kartą."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "Atnaujinimas neteisingai pasirašytas."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Yra prieinama nauja %s versija!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Gauti atnaujinimą"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Yra %s %s versija (jūs naudojate %s). Ar norite ją dabar parsisiųsti?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Siunčiamas atnaujinimas..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s iš %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Paruošta diegimui."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Ieškoti atnaujinimų automatiškai?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Norite, kad %s automatiškai tikrintų, ar yra atnaujinimų? Visada galite ieškoti naujinimų rankiniu būdu iš meniu."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Ieškoti automatiškai"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Neieškoti"

