#: src/ui.cpp:325 src/ui.cpp:674 src/ui.cpp:690
msgid "Software Update"
msgstr "Software-Aktualisierung"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Versionshinweise:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Diese Version überspringen"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Später erinnern"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Update installieren"

#: src/ui.cpp:673
#, c-format
msgid "%s cannot be restarted."
msgstr "%s kann nicht neu gestartet werden."

#: src/ui.cpp:676
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Stellen Sie sicher, dass Sie keine ungespeicherten Dokumente haben und versuchen Sie es erneut."

#: src/ui.cpp:683
msgid "Launching the installer..."
msgstr "Installationsprogramm wird gestartet …"

#: src/ui.cpp:689
msgid "Failed to launch the installer."
msgstr "Starten des Installationsprogramms fehlgeschlagen."

#: src/ui.cpp:732
msgid "Checking for updates..."
msgstr "Es wird auf Aktualisierungen geprüft …"

#: src/ui.cpp:734 src/ui.cpp:815 src/ui.cpp:903
msgid "Cancel"
msgstr "Abbrechen"

#: src/ui.cpp:760
msgid "You're up to date!"
msgstr "Sie benutzen bereits die neueste Version!"

#: src/ui.cpp:767
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s ist zur Zeit die neueste verfügbare Version."

#: src/ui.cpp:780
msgid "Close"
msgstr "Schließen"

#: src/ui.cpp:801
msgid "Update Error!"
msgstr "Fehler bei der Aktualisierung!"

#: src/ui.cpp:807
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Beim Abrufen der Aktualisierungsinformationen ist ein Fehler aufgetreten; Stellen Sie sicher, dass Sie mit dem Internet verbunden sind und versuchen Sie es dann erneut."

#: src/ui.cpp:810
msgid "The update is improperly signed."
msgstr "Das Update ist nicht ordnungsgemäß signiert."

#: src/ui.cpp:861
#, c-format
msgid "A new version of %s is available!"
msgstr "Eine neue Version von %s ist verfügbar!"

#: src/ui.cpp:864
msgid "Get update"
msgstr "Jetzt aktualisieren"

#: src/ui.cpp:870
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s ist jetzt verfügbar (Sie haben %s). Möchten Sie die neue Version jetzt herunterladen?"

#: src/ui.cpp:901
msgid "Downloading update..."
msgstr "Update wird heruntergeladen..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:929
#, c-format
msgid "%s of %s"
msgstr "%s von %s"

#: src/ui.cpp:966
msgid "Ready to install."
msgstr "Bereit zur Installation."

#: src/ui.cpp:1111
msgid "Check for updates automatically?"
msgstr "Automatisch nach Aktualisierungen suchen?"

#: src/ui.cpp:1121
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Soll %s automatisch nach Aktualisierungen suchen? Sie können vom Menü aus jederzeit manuell nach Aktualisierungen suchen."

#: src/ui.cpp:1135
msgid "Check automatically"
msgstr "Automatisch überprüfen"

#: src/ui.cpp:1140
msgid "Don't check"
msgstr "Nicht überprüfen"

