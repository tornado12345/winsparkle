#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Programmatuurbywerking"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Vrystellingsnotas:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Slaan hierdie weergawe oor"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Herinner my later"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Installeer bywerking"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s kan nie herbegin word nie."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Maak seker daar is geen onbewaarde dokumente is nie en probeer weer."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Lanseer tans die installeerder…"

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Installeerder kon nie gelanseer word nie."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Soek tans na bywerkings…"

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Kanselleer"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "U is bygewerk!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s is tans die nuutste beskikbare weergawe."

#: src/ui.cpp:774
msgid "Close"
msgstr "Maak toe"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Bywerkfout!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "’n Fout het voorgekom tydens die ophalen van bywerkinligting; is u verbind aan die internet? Probeer later weer."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "Die bywerking se digitale ondertekening is ongeldig."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "’n Nuwe weergawe van %s is beskikbaar!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Kry bywerking"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s is nou beskikbaar (u het %s). Wil u dit nou aflaai?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Laai tans bywerking af…"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s van %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Gereed om te installeer."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Soek outomaties na bywerkings?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Moet %s outomaties na bywerkings soek? U kan altyd self vanuit die kieslys na bywerkings soek."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Soek outomaties"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Moenie soek nie"

