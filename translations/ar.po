#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "تحديث البرمجيّات"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "ملاحظات الإصدار:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "تخطّ هذه النّسخة"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "ذكّرني لاحقًا"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "ثبّت التّحديث"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "تعذّرت إعادة تشغيل %s."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "تأكّد من عدم وجود مستندات غير محفوطة وجرّب مجدّدًا."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "يُطلق المثبّت..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "فشل إطلاق المثبّت."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "يلتمس التّحديثات..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "ألغِ"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "البرنامج محدّث!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "النّسخة %s من %s هي أحدث واحدة متوفّرة."

#: src/ui.cpp:774
msgid "Close"
msgstr "أغلق"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "خطأ في التّحديث!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "حصل خطأ في جلب معلومات التّحديث، أأنت متّصل بالإنترنت؟ رجاء جرّب مجدّدًا لاحقًا."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "تم توقيع التحديث بشكل غير صحيح."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "ثمّة نسخة جديدة من %s متوفّرة!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "احصل على التّحديث"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "النّسخة %s من %s متوفّرة الآن (لديك أنت %s). أترغب بتنزيلها الآن؟"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "ينزّل التّحديث..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s من %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "جاهز للتّثبيت."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "أألتمس التّحديثات آليًّا؟"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "هل على %s التماس التّحديثات آليًّا؟ يمكنك دائمًا التماس التّحديثات يدويًّا من القائمة."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "التمس آليًّا"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "لا تلتمس"

