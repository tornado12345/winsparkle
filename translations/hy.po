#: src/ui.cpp:325 src/ui.cpp:674 src/ui.cpp:690
msgid "Software Update"
msgstr "Ծրագրի թարմացում"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Notes de version:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Skip this version"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Հիշեցնել ավելի ուշ"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Տեղադրել թարմացումը"

#: src/ui.cpp:673
#, c-format
msgid "%s cannot be restarted."
msgstr "%s-ը չի կարող վերագործարկվել"

#: src/ui.cpp:676
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Համոզվեք, որ չունեք չպահպանված փաստաթուղթ և կրկին փորձեք:"

#: src/ui.cpp:683
msgid "Launching the installer..."
msgstr "Բացում է տեղակայիչը..."

#: src/ui.cpp:689
msgid "Failed to launch the installer."
msgstr "Չհաջողվեց բացել տեղակայիչը:"

#: src/ui.cpp:732
msgid "Checking for updates..."
msgstr "Թարմացումների ստուգում..."

#: src/ui.cpp:734 src/ui.cpp:815 src/ui.cpp:903
msgid "Cancel"
msgstr "Չեղարկել"

#: src/ui.cpp:760
msgid "You're up to date!"
msgstr "Ծրագրի նոր տարբերակ չկա:"

#: src/ui.cpp:767
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s-ը վերջին տարբերակն է:"

#: src/ui.cpp:780
msgid "Close"
msgstr "Փակել"

#: src/ui.cpp:801
msgid "Update Error!"
msgstr "Թարմացման սխալ"

#: src/ui.cpp:807
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Սխալ՝ թարմացման տեղեկությունները ստանալիս: Մուտք կա՞ համացանց: Խնդրում ենք կրկին փորձել:"

#: src/ui.cpp:810
msgid "The update is improperly signed."
msgstr ""

#: src/ui.cpp:861
#, c-format
msgid "A new version of %s is available!"
msgstr "Հասանելի է %s-ի նոր տարբերակը"

#: src/ui.cpp:864
msgid "Get update"
msgstr "Թարմացնել"

#: src/ui.cpp:870
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "Հասանելի է %s %s-ը (ընթացիկը՝ %s): Ներբեռնե՞լ այն:"

#: src/ui.cpp:901
msgid "Downloading update..."
msgstr "Թարմացումների ներբեռնում..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:929
#, c-format
msgid "%s of %s"
msgstr "%s %s-ից"

#: src/ui.cpp:966
msgid "Ready to install."
msgstr "Պատրաստ է տեղադրման համար"

#: src/ui.cpp:1111
msgid "Check for updates automatically?"
msgstr "Ստուգե՞լ թարմացումները ինքնաբար"

#: src/ui.cpp:1121
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "%s-ը ինքնաբա՞ր ստուգի թարմացումները: Ցանկացած ժամանակ կարող եք ինքներդ ստուգել:"

#: src/ui.cpp:1135
msgid "Check automatically"
msgstr "Ստուգել ինքնաբար"

#: src/ui.cpp:1140
msgid "Don't check"
msgstr "Չստուգել"

