#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Actualització de programari"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Notes de la versió:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Omet aquesta versió"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Recorda-m’ho més tard"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instal·la l’actualització"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "No s’ha pogut reiniciar %s."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Assegureu-vos que no teniu cap document sense desar i torneu a provar-ho."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "S’està obrint l’instal·lador…"

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "No s’ha pogut obrir l’instal·lador."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "S’està comprovant si hi ha actualitzacions…"

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Cancel·la"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Ja teniu la versió més recent."

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s és la versió més nova disponible ara."

#: src/ui.cpp:774
msgid "Close"
msgstr "Tanca"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Error d'actualització!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "No s’ha pogut recuperar la informació d’actualitzacions. Comproveu que la vostra connexió d’Internet estigui activa i proveu un altre cop més tard."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "L’actualització s’ha signat incorrectament."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Una nova versió de %s està disponible!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Aconseguir actualització"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s està disponible (teniu la versió %s). Voleu baixar-la ara?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "S’està baixant l’actualització…"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s de %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "La instal·lació és a punt."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Voleu comprovar si hi ha actualitzacions automàticament?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Voleu que %s comprovi si hi ha actualitzacions automàticament? Sempre podreu fer-ho manualment des del menú."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Comprova automàticament"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "No comprovis res"

