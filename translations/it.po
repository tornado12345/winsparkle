#: src/ui.cpp:325 src/ui.cpp:674 src/ui.cpp:690
msgid "Software Update"
msgstr "Aggiornamento software"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Note di versione:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Ignora questa versione"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Ricordamelo più tardi"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Installa aggiornamento"

#: src/ui.cpp:673
#, c-format
msgid "%s cannot be restarted."
msgstr "%s non può essere riavviato."

#: src/ui.cpp:676
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Assicurarsi di aver salvato tutti i documenti e riprovare."

#: src/ui.cpp:683
msgid "Launching the installer..."
msgstr "Avvio dell'installazione in corso..."

#: src/ui.cpp:689
msgid "Failed to launch the installer."
msgstr "Non posso avviare l'installazione."

#: src/ui.cpp:732
msgid "Checking for updates..."
msgstr "Verifico aggiornamenti..."

#: src/ui.cpp:734 src/ui.cpp:815 src/ui.cpp:903
msgid "Cancel"
msgstr "Annulla"

#: src/ui.cpp:760
msgid "You're up to date!"
msgstr "Programma aggiornato!"

#: src/ui.cpp:767
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s è l'ultima versione disponibile."

#: src/ui.cpp:780
msgid "Close"
msgstr "Chiudi"

#: src/ui.cpp:801
msgid "Update Error!"
msgstr "Aggiornamento non riuscito!"

#: src/ui.cpp:807
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Si è verificato un errore durante il recupero delle informazioni di aggiornamento. Verifica la connessione ad Internet e riprova."

#: src/ui.cpp:810
msgid "The update is improperly signed."
msgstr ""

#: src/ui.cpp:861
#, c-format
msgid "A new version of %s is available!"
msgstr "È disponibile una nuova versione di %s!"

#: src/ui.cpp:864
msgid "Get update"
msgstr "Ottieni aggiornamento"

#: src/ui.cpp:870
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s è disponibile (versione attuale %s). Scaricare ora?"

#: src/ui.cpp:901
msgid "Downloading update..."
msgstr "Scaricamento aggiornamento in corso..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:929
#, c-format
msgid "%s of %s"
msgstr "%s di %s"

#: src/ui.cpp:966
msgid "Ready to install."
msgstr "Pronto per l'installazione."

#: src/ui.cpp:1111
msgid "Check for updates automatically?"
msgstr "Verificare gli aggiornamenti automaticamente?"

#: src/ui.cpp:1121
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Lasciare a %s la verifica automatica degli aggiornamenti? È sempre possibile verificare manualmente."

#: src/ui.cpp:1135
msgid "Check automatically"
msgstr "Verifica automaticamente"

#: src/ui.cpp:1140
msgid "Don't check"
msgstr "Non verificare"

