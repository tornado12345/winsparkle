#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Actualización de software"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Informe de cambios"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Omitir esta versión"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Recordármelo más tarde"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instalar actualización"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "No se puede reiniciar %s."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Asegúrate de que no tienes ningún documento sin guardar y vuelve a intentarlo."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Iniciando la instalación..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "No se pudo iniciar la instalación."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Buscando actualizaciones…"

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Cancelar"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "¡Ya estás actualizado!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s es actualmente la versión más reciente disponible."

#: src/ui.cpp:774
msgid "Close"
msgstr "Cerrar"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "¡Error al actualizar!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Se produjo un error al recuperar información de la actualización. Revise su conexión a Internet e inténtelo de nuevo más tarde."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "La actualización no está firmada adecuadamente."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "¡Una nueva versión de %s está disponible!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Actualizar"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s ya está disponible (su versión es %s). ¿Le gustaría descargarla ahora?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Descargando actualización..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s de %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Listo para instalar."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "¿Comprobar si hay actualizaciones automáticamente?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "¿Debe %s comprobar las actualizaciones automáticamente? Puedes comprobar las actualizaciones manualmente desde el menú."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Comprobar automáticamente"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "No comprobar"

