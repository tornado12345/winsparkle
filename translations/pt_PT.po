#: src/ui.cpp:325 src/ui.cpp:674 src/ui.cpp:690
msgid "Software Update"
msgstr "Atualização do software"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Notas de lançamento:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Ignorar esta versão"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Lembrar mais tarde"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instalar atualização"

#: src/ui.cpp:673
#, c-format
msgid "%s cannot be restarted."
msgstr "%s não pode ser reiniciado."

#: src/ui.cpp:676
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Certifique-se que não tem documentos por guardar e tente novamente."

#: src/ui.cpp:683
msgid "Launching the installer..."
msgstr "A iniciar o instalador..."

#: src/ui.cpp:689
msgid "Failed to launch the installer."
msgstr "Falha ao iniciar o instalador."

#: src/ui.cpp:732
msgid "Checking for updates..."
msgstr "A procurar por atualizações..."

#: src/ui.cpp:734 src/ui.cpp:815 src/ui.cpp:903
msgid "Cancel"
msgstr "Cancelar"

#: src/ui.cpp:760
msgid "You're up to date!"
msgstr "A sua versão está atualizada!"

#: src/ui.cpp:767
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s é a versão mais recente disponível."

#: src/ui.cpp:780
msgid "Close"
msgstr "Fechar"

#: src/ui.cpp:801
msgid "Update Error!"
msgstr "Erro ao atualizar!"

#: src/ui.cpp:807
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Ocorreu um erro ao obter as informações da atualização. Está ligado à Internet? Por favor tente novamente."

#: src/ui.cpp:810
msgid "The update is improperly signed."
msgstr "A atualização não possui um assinatura válida."

#: src/ui.cpp:861
#, c-format
msgid "A new version of %s is available!"
msgstr "Está disponível uma nova versão do %s!"

#: src/ui.cpp:864
msgid "Get update"
msgstr "Obter atualização"

#: src/ui.cpp:870
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s está disponível (a sua versão é a %s). Gostaria de descarregar a nova versão?"

#: src/ui.cpp:901
msgid "Downloading update..."
msgstr "A descarregar a atualização..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:929
#, c-format
msgid "%s of %s"
msgstr "%s de %s"

#: src/ui.cpp:966
msgid "Ready to install."
msgstr "Pronto a instalar."

#: src/ui.cpp:1111
msgid "Check for updates automatically?"
msgstr "Procurar atualizações automaticamente?"

#: src/ui.cpp:1121
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Pretende que o %s procure atualizações automaticamente? Também pode procurar as atualizações a partir do menu."

#: src/ui.cpp:1135
msgid "Check automatically"
msgstr "Procurar automaticamente"

#: src/ui.cpp:1140
msgid "Don't check"
msgstr "Não procurar"

